{% extends 'base.html.twig' %}

{% block title %}Hello IndexController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
<table>
<thead>
    <th>user id</th>
<th>username</th>
    <th>userage</th>
        <th>userprojecttitle</th>
            <th>userprojectdesc</th>
</thead>

<tbody>
    <tr>
{% for user in table %}
<td>{{user.id}}</td>
<td>{{user.username}}</td>
<td>{{user.userage}}</td> 
{% for project in user.userprojects %}
<td>{{project.userprojecttitle}}</td>
<td>{{project.userprojectdesc}}</td>
</tr>
{% endfor %}   

{% endfor %}

</tbody>

</table>




</div>
{% endblock %}
